@model List<CoreYiyecekler.Data.Models.Category>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Category List</h1>
<br />

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Category Name</th>
          <th>Status</th>
        <th>Delete</th>
        <th>Update</th>
      
    <tr />
    @foreach (var x in Model)
    {



        <tr>
            <td>@x.CategoryID</td>
            <td>@x.CategoryName</td>
            <td>@x.Status</td>
            <td><a href="#" class="btn btn-danger">Delete</a></td>
            <td><a href="#" class="btn btn-info">Update</a></td>

        </tr>

    }

</table>

<a href="/Category/CategoryAdd" class="btn btn-info">New Category</a>



